<div class="modal fade" id="AgregarContenido" role="dialog">
  <div class="modal-dialog">
    <!-- Modal content-->
    <div class="modal-content">
      <div class="modal-header">
        <h4 class="modal-title">Agregar Contenido</h4>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <div class="row m-2">
          <h6>Descripcion</h6>
          <input type="text" class="form-control" name="" value="" id="txtDescripcionAgregarContenido">
        </div>
        <div class="row m-2">
          <h6>Ejercicio</h6>
          <input type="text" class="form-control" name="" value="" id="txtEjercicioAgregarContenido">
        </div>
        <div class="modal-footer">
          <div class="m-2 d-flex flex-wrap justify-content-end">
              <button id="btnAgregarContenido" type="button" class="btn btn-primary mx-2" name="button">Agregar</button>
              <button id="btnCancelarContenido" type="button" class="btn btn-secondary" name="button" data-dismiss="modal">Cancelar</button>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
